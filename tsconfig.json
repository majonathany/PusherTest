{
  "compilerOptions": {
    "outFile": "bundle.js",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
      "~*": ["./*"]
    },
    "include": ["src/**/*"]

  }
}
